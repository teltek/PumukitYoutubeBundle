<div class="well">
    <table class="table table-striped table-hover pmk-tag-tree">
        <thead>
            <tr>
                <th id="th-checkbox" width="1%">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" onclick="window.click_checkbox_all('tag', this.checked)">
                        </label>
                    </div>
                </th>
                <th id="th-icons" colspan="3" width="3%"></th>
                <th id="th-id" width="1%">{% trans %}Id{% endtrans %}</th>
                <th id="th-code-name">{% trans %}Name{% endtrans %} - {% trans %}Login{% endtrans %}</th>
                <th id="th-num">{% trans %}MM. Obj.{% endtrans %}</th>
            </tr>
        </thead>
        <tbody>
            {% include 'PumukitYoutubeBundle:Admin:children.html.twig' %}
        </tbody>
        <tfoot>
            <tr>
                <td colspan="8" class="foot-td">
                    &nbsp;
                </th>
            </tr>
        </tfoot>
    </table>
</div>